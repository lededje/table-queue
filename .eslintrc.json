{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": false,
    "import/extensions": false,
    "no-console": [1, { "allow": ["warn", "error"] }],
    "import/prefer-default-export": false
  },
  "globals": {
    "window": true,
    "jest": true,
    "describe": true,
    "expect": true,
    "it": true,
    "beforeEach": true,
    "afterEach": true
  }
}
